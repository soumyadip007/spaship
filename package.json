{
  "name": "@spaship/spaship",
  "description": "Monorepo for the SPAship project.",
  "version": "23.05.0",
  "private": true,
  "devDependencies": {
    "lerna": "5.6.2"
  },
  "scripts": {
    "start:api": "cd ./packages/api && npm run start",
    "start:manager": "cd ./packages/manager && npm run start",
    "dev:api": "cd ./packages/api && npm run start:dev",
    "dev:manager": "cd ./packages/manager && npm run dev",
    "lerna": "./node_modules/.bin/lerna"
  }
}